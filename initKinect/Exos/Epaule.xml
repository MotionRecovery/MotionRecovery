<?xml version="1.0" encoding="utf-8" ?>
<ExerciseRule Name="Epaule">
	<description>"Exercice to recover the elbow"</description>
	<Position>
		<Membre1>ElbowRight</Membre1>
		<Membre2>WristRight</Membre2>
		<Membre3>HandRight</Membre3>
		<AngleMin>75,0</AngleMin>
		<AngleMax>100,0</AngleMax>
		<Description>"The right wrist should be betwen 75 and 100 degrees"</Description>
	</Position>
	<Position>
		<Membre1>Head</Membre1>
		<Membre2>Neck</Membre2>
		<AngleMin>80,0</AngleMin>
		<AngleMax>100,0</AngleMax>
		<Description>"The head should be at 80 and 90 degrees"</Description>
	</Position>
</ExerciseRule>
