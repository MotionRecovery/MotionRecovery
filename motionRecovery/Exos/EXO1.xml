<?xml version="1.0" encoding="utf-8" ?>
<ExerciseRule Name="EXO1">
  <name>EXO1</name>
  <description>"The first Exercice"</description>
  <rule>
    <Position>
      <Membre1>ElbowRight</Membre1>
      <Membre2>WristRight</Membre2>
      <AngleMin>75,0</AngleMin>
      <AngleMax>100,0</AngleMax>
    </Position>
    <Position>
      <Membre1>ElbowLeft</Membre1>
      <Membre2>WristLeft</Membre2>
      <AngleMin>75,0</AngleMin>
      <AngleMax>100,0</AngleMax>
    </Position>
    <PositionTime>15,0</PositionTime>
    <Description>
      [Right and left] Keep the wrist in relation to the elbow between 75 and 100 degrees
    </Description>
  </rule>
  <rule>
    <Position>
      <Membre1>Neck</Membre1>
      <Membre2>Head</Membre2>
      <AngleMin>75,0</AngleMin>
      <AngleMax>100,0</AngleMax>
    </Position>
    <PositionTime>15,0</PositionTime>
    <Description>
      Neck and Head, 75-100°
    </Description>
  </rule>
</ExerciseRule>